{
  "name": "tracker",
  "version": "2.5.1",
  "description": "Application UI for PRO MERN Stack development",
  "main": "index.js",
  "scripts": {
    "#start": "UI Server. HMR is enabled in dev mode",
    "start": "node dist/server.js",
    "#lint": "Runs ESLint on all relevant files",
    "lint": "eslint server src browser --ext js,jsx",
    "#compile": "Generates JS bundles for production. Use with start",
    "compile": "webpack --mode production",
    "#watch-server-hmr": "Recompile server HMR bundle on change.",
    "watch-server-hmr": "webpack -w --config webpack.serverHMR.js",
    "#dev-all": "Dev-Mode: uses concurrently to run 2 commands at once",
    "dev-all": "npm run watch-server-hmr & timeout 5 && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petelc/Issue-Tracker.git"
  },
  "author": "Peter Carroll <petelc1966@hotmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/petelc/Issue-Tracker",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.5.2",
    "clsx": "^1.1.1",
    "concurrently": "^5.3.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "fontsource-roboto": "^2.2.6",
    "http-proxy-middleware": "^0.21.0",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "source-map-support": "^0.5.19",
    "url-search-params": "^1.1.0",
    "whatwg-fetch": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^2.5.1"
  }
}
